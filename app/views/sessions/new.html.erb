<h1>Login</h1>

<%= form_with(url: login_path, method: :post) do |form| %>
  <%= form.label :username %>
  <%= form.text_field :username, name: 'session[username]' %>

  <%= form.label :password %>
  <%= form.password_field :password, name: 'session[password]' %>

  <%= form.submit 'Login' %>
<% end %>
